{
  "name": "elm-pages-example",
  "version": "1.0.0",
  "description": "Example site built with elm-pages.",
  "scripts": {
    "watch": "elm-pages --watch",
    "start": "concurrently \"npm:watch\" \"parcel src/index.html\"",
    "start:prod": "npm run build && http-server ./dist -a localhost -p 3000 -c-1",
    "build": "rm -rf dist && parcel build src/index.html",
    "test": "elm-test",
    "autotest": "elm-test --watch"
  },
  "author": "Dillon Kearns",
  "license": "BSD-3",
  "dependencies": {},
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^12.6.8",
    "concurrently": "^4.1.1",
    "elm": "^0.19.0-no-deps",
    "elm-hot": "^1.0.1",
    "elm-pages": "^1.0.4",
    "elm-test": "^0.19.0-rev6",
    "node-elm-compiler": "^5.0.3",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-prerender": "^1.4.1",
    "typescript": "^3.5.3"
  }
}
